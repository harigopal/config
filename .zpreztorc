# Add the git, syntax-highlighting, and autosuggestions modules to load order.
zstyle ':prezto:load' pmodule \
  'environment' \
  'terminal' \
  # ...other modules
  'prompt' \
  'git' \
  'syntax-highlighting' \
  'autosuggestions'

# Use the 'powerlevel10k' theme.
zstyle ':prezto:module:prompt' theme 'powerlevel10k'
